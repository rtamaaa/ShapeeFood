  UStatusPesanan in 'UStatusPesanan.pas' {StatusPemesanan},
  UDashboard in 'UDashboard.pas' {Dashboard},
  UKalkulator in '..\kalkulator\UKalkulator.pas' {Kalkulator},
  UKAlkulatorBangun in '..\Penggabungan\Kalkulator Bangun 2D dan 3D\UKAlkulatorBangun.pas' {FKalkulatorBangun},
  Ugaji in '..\Penggabungan\hitung gaji karyawan\Ugaji.pas' {Fgaji1},
