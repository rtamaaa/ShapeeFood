  Application.CreateForm(TFgaji1, Fgaji1);
  Application.CreateForm(Tfunction1, function1);
  Application.CreateForm(TFLooping, FLooping);
  Application.CreateForm(TFArray, FArray);
  Application.CreateForm(TFRestoran, FRestoran);
  Application.CreateForm(TFLogin, FLogin);
  Application.Run;
